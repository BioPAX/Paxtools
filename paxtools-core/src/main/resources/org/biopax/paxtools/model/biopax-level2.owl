  <owl:Class rdf:about="#chemicalStructure">
    <rdfs:subClassOf rdf:resource="#utilityClass"/>
    <owl:disjointWith rdf:resource="#confidence"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:ID="STRUCTURE-DATA"/>
        </owl:onProperty>
