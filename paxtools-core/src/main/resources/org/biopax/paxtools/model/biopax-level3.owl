  <owl:Class rdf:ID="Modulation">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#Catalysis"/>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:about="#controlled"/>
