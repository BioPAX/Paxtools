  <owl:Class rdf:about="#Transport">
    <owl:disjointWith rdf:resource="#Degradation"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#ComplexAssembly"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
