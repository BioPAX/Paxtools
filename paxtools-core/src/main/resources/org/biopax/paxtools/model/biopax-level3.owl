        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#xref"/>
        </owl:onProperty>
        <owl:allValuesFrom>
          <owl:Class>
            <owl:unionOf rdf:parseType="Collection">
              <owl:Class rdf:ID="PublicationXref"/>
              <owl:Class rdf:about="#UnificationXref"/>
            </owl:unionOf>
          </owl:Class>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
