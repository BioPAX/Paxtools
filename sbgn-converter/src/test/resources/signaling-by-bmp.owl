<bp:BiochemicalReaction rdf:about="http://identifiers.org/reactome/R-HSA-201457">
 <bp:xref rdf:resource="#UnificationXref_reactome_R-HSA-201457" />
 <bp:displayName rdf:datatype = "http://www.w3.org/2001/XMLSchema#string">BMP2 binds to the receptor complex</bp:displayName>
 <bp:right rdf:resource="#Complex_730a4d4594f7c28c24a6b43a82c2d27b" />
 <bp:left rdf:resource="#Complex_efcb1551e9c9be269c30903dd412b2c3" />
 <bp:left rdf:resource="#Complex_14aa57f119128f9888d3bea4ea00b299" />
 <bp:comment rdf:datatype = "http://www.w3.org/2001/XMLSchema#string">Authored: Huminiecki, L, Moustakas, A, 2007-11-07 10:22:00</bp:comment>
