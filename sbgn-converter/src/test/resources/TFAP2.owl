<bp:ProteinReference rdf:about="http://identifiers.org/uniprot/Q99967">
 <bp:standardName rdf:datatype = "http://www.w3.org/2001/XMLSchema#string">Cbp/p300-interacting transactivator 2</bp:standardName>
