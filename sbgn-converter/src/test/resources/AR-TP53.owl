<bp:ProteinReference rdf:about="urn:miriam:uniprot:P04899">
 <bp:name rdf:datatype = "http://www.w3.org/2001/XMLSchema#string">GNAI2</bp:name>
 <bp:name rdf:datatype = "http://www.w3.org/2001/XMLSchema#string">Adenylate cyclase-inhibiting G alpha protein</bp:name>
 <bp:xref rdf:resource="urn:biopax:UnificationXref:UNIPROT_P04899" />
 <bp:xref rdf:resource="urn:biopax:RelationshipXref:REFSEQ_NP_002061" />
 <bp:xref rdf:resource="urn:biopax:UnificationXref:UNIPROT_Q8IZ71" />
