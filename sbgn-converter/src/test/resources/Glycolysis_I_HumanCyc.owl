 <bp:xref rdf:resource="http://identifiers.org/pubmed/19786097" />
 <bp:xref rdf:resource="http://identifiers.org/pubmed/2579079" />
 <bp:xref rdf:resource="http://identifiers.org/pubmed/4022011" />
